{
  "name": "react-resume-template",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  },
  "homepage": "https://zinduolis.github.io/redgraz-cv/",
  "scripts": {
    "build": "yarn compile && yarn next build",
