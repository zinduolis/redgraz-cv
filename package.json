    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "yet-another-react-lightbox": "^3.23.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.18",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
