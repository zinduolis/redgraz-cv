    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.82.0",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.2",
